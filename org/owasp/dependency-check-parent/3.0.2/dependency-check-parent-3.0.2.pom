            </dependency>
            <dependency>
                <groupId>org.apache.maven.reporting</groupId>
                <artifactId>maven-reporting-api</artifactId>
                <version>3.0</version>
            </dependency>
            <!-- Upgrading transitive commons-collections-3.2.1 from velocity-1.7. -->
